{
    "name": "PyTorch Serve",
    "slug": "torch-serve",
    "category": "serve",
    "table_field": {
        "url": "https://{{ release }}.{{ global.domain }}/predictions/"
    },
    "description": "Etiam rhoncus. Maecenas tempus, tellus condimentum rhoncus, sem quam libero, sit amet adipiscing sem neque sed ipsum. Nam quam nunc, vel.",
    "settings": {
        "logs": ["serve", "gatekeeper"],
        "permissions": {
            "public": {"value":"false", "option": "true"},
            "project": {"value":"true", "option": "true"},
            "private": {"value":"false", "option": "true"}
        },
        "flavor": "one",
        "apps": {
            "Persistent Volume":"one"
        },
        "modelstore": {
            "meta": {
                "title": "Models"
            },
            "path": {
                "type": "string",
                "default": "model-store",
                "title": "Path to Model Store"
            },
            "models": {
                "type": "string",
                "default": "",
                "title": "Comma-separeted list of models (model1,model2,model3)" 
            }
        },
        "env_variables": {
            "{{ app_name }}_URL": "https://{{ release }}.{{ global.domain }}/predictions/"
        }
    }

}