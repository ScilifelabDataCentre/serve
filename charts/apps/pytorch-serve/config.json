{
    "name": "PyTorch Serve",
    "slug": "torch-serve",
    "category": "serve",
    "table_field": {
        "url": "https://{{ release }}.{{ global.domain }}/predictions/"
    },
    "description": "",
    "priority": "500",
    "settings": {
        "logs": ["serve"],
        "model": {
            "object_type": "pytorch"
        },
        "permissions": {
            "public": {"value":"false", "option": "true"},
            "project": {"value":"true", "option": "true"},
            "private": {"value":"false", "option": "true"}
        },
        "flavor": "one",
        "default_values": {
            "port": "80",
            "targetport": "8080"
          },
        "modelstore": {
            "meta": {
                "title": "Models"
            },
            "path": {
                "type": "string",
                "default": "models",
                "title": "S3 bucket"
            }
        },
        "env_variables": {
            "{{ app_name }}_URL": "https://{{ release }}.{{ global.domain }}/predictions/"
        }
    }

}